/* @bsp, 2020/05/27 Battery & Charging porting */
&tlmm {
	oneplus_fastchg {
		usb_sw_active: usb_sw_active {
			mux {
				pins = "gpio12", "gpio48";
				function = "gpio";
			};

			config {
				pins = "gpio12", "gpio48";
				drive-strength = <16>;
				bias-pull-down;
			};
		};

		usb_sw_suspend: usb_sw_suspend {
			mux {
				pins = "gpio12", "gpio48";
				function = "gpio";
			};

			config {
				pins = "gpio12", "gpio48";
				drive-strength = <2>;
				bias-disable;
			};
		};

		fingerprint_detect{
			fp_id_init: fp_id_init {
				mux {
				    pins = "gpio55";
				    function = "gpio";
				};
				config {
					pins = "gpio55";
					drive-strength = <2>;
					bias-diable;
					output-high;
				};
			};
		};

		fastchg_active: fastchg_active {
			mux {
				pins = "gpio141";
				function = "gpio";
			};

			config {
				pins = "gpio141";
				drive-strength = <16>;
				bias-pull-up;
			};
		};

		fastchg_suspend: fastchg_suspend {
			mux {
				pins = "gpio141";
				function = "gpio";
			};

			config {
				pins = "gpio141";
				drive-strength = <2>;
				bias-disable;
			};
		};

		ap_clk_active: ap_clk_active {
			mux {
				pins = "gpio85";
				function = "gpio";
			};

			config {
				pins = "gpio85";
				drive-strength = <16>;
				bias-pull-up;
			};
		};

		ap_clk_suspend: ap_clk_suspend {
			mux {
				pins = "gpio85";
				function = "gpio";
			};

			config {
				pins = "gpio85";
				drive-strength = <2>;
				bias-disable;
			};
		};

		ap_data_active: ap_data_active {
			mux {
				pins = "gpio84";
				function = "gpio";
			};

			config {
				pins = "gpio84";
				drive-strength = <16>;
				bias-pull-up;
			};
		};

		ap_data_suspend: ap_data_suspend {
			mux {
				pins = "gpio84";
				function = "gpio";
			};

			config {
				pins = "gpio84";
				drive-strength = <2>;
				bias-disable;
			};
		};
	};
	
	rf_cable_ant1_active: rf_cable_ant1_active {
            mux {
                pins = "gpio108";
                function = "gpio";
            };
            config {
                pins = "gpio108";
                drive-strength = <2>;
                bias-pull-up;
            };
        };
        rf_cable_ant2_active: rf_cable_ant2_active {
            mux {
                pins = "gpio123";
                function = "gpio";
            };
            config {
                pins = "gpio123";
                drive-strength = <2>;
                bias-pull-up;
            };
        };
        rf_cable_ant3_active: rf_cable_ant3_active {
            mux {
                pins = "gpio54";
                function = "gpio";
            };
            config {
                pins = "gpio54";
                drive-strength = <2>;
                bias-pull-up;
            };
        };
	rf_cable_ant4_active: rf_cable_ant4_active {
            mux {
                pins = "gpio112";
                function = "gpio";
            };
            config {
                pins = "gpio112";
                drive-strength = <2>;
                bias-pull-up;
            };
        };
